#!/bin/bash

cd deploy
rm -rf bin
rm -rf dev/krushwebapi
cp -r ../bin bin
cp -r ../dev/krushwebapi dev
~/util/hyper run --detach --name krushwebapi --expose 8080 --expose 8793 --expose 3000 -p 8080:8080 -p 8793:8793 -p 3000:3000 --volume `pwd`:/docked desavera/krushweb:krushwebapi /docked/bin/krushwebapi.run
~/util/hyper fip attach 162.221.195.113 krushwebapi
